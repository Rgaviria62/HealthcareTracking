            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.healthcaretracking.Home"
            android:label="@string/title_activity_home" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.healthcaretracking.Aboutme"
            android:label="@string/title_activity_aboutme"
            android:parentActivityName="com.example.healthcaretracking.Home" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthcaretracking.Home" />
        </activity>
