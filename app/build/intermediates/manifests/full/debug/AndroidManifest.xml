                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthcaretracking.Home" />
        </activity>
        <activity
            android:name="com.example.healthcaretracking.Dieta"
            android:label="@string/title_activity_dieta"
            android:parentActivityName="com.example.healthcaretracking.Home" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthcaretracking.Home" />
        </activity>
        <activity
            android:name="com.example.healthcaretracking.Medicamentosprogramados"
            android:label="@string/title_activity_tratamiento"
